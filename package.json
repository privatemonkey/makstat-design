{
  "name": "makstat-design",
  "version": "1.0.0",
  "description": "Design templates and code for MakStat website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch scss/main.scss:assets/css/style.css",
    "devserver": "http-server -c-1 --proxy http://localhost:8080?",
    "start": "npm-run-all -p devserver watch:sass",
    "dev": "npm-run-all -p devserver watch:sass",
    "compile:sass": "sass scss/main.scss:assets/css/style.css",
    "compress:css": "node-sass assets/css/style.css assets/css/style.min.css --output-style compressed",
    "build": "npm-run-all -s compile:sass compress:css"
  },
  "keywords": [],
  "author": "Anders Olsen",
  "license": "MIT",
  "dependencies": {
    "http-server": "^14.1.1",
    "node-sass": "^7.0.3",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
